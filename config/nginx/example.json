{
  "webSite": {
    "type": "php",
    "port": "80",
    "name": "www.example.com",
    "root": "/var/www/example/site_folder",
    "locations": {
      "root": {
        "path": "/"
      }
    },
    "rewriteLocations": {
      "Downloads": {
        "path": "/exampleURL",
        "destination": "/correctURL"
      }
    },
    "redirectLocations": {
      "Downloads" : {
        "path": "/downloads",
        "uri": "downloads.example.com"
      }
    }
  },
}