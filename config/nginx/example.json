{
  "webSite": {
    "type": "php",
    "port": "80",
    "name": "www.example.com",
    "root": "/var/www/example/site_folder",
    "locations": [
      {
        "path": "/"
      }
    ],
    "rewriteLocations": [
      {
        "path": "/exampleURL",
        "destination": "/correctURL"
      }
    ],
    "redirectLocations": [
      {
        "path": "/downloads",
        "uri": "downloads.example.com"
      }
    ]
  }
}